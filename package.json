{
  "name": "monolieta-search",
  "version": "1.2.2",
  "description": "Search library for browser and Node.js",
  "main": "./dist/monolieta-search.umd.js",
  "module": "./dist/monolieta-search.es.js",
  "exports": {
    ".": {
      "import": "./dist/monolieta-search.es.js",
      "require": "./dist/monolieta-search.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite",
    "format": "prettier --write .",
    "lint": "prettier --check .",
    "preview": "vite preview",
    "public": "npm run build && npm publish --access public",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "~29.5.3",
    "jest": "~29.6.1",
    "prettier": "~3.0.0",
    "ts-jest": "~29.1.1",
    "typescript": "~5.1.6",
    "vite": "~4.4.4",
    "vite-plugin-dts": "~3.2.0"
  },
  "keywords": [
    "search",
    "performance",
    "indexing"
  ],
  "license": "MIT",
  "author": "Jonattan Vel√°squez <jonattanva89@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/jonattanva/monolieta-search.git"
  },
  "engines": {
    "node": ">=18.x"
  }
}
