{
  "name": "monolieta-search",
  "version": "1.2.3",
  "description": "Search library for browser and Node.js",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "preview": "vite preview",
    "public": "npm run build && npm publish --access public",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "~7.15.0",
    "@typescript-eslint/parser": "~8.30.1",
    "@vitest/coverage-v8": "~3.0.5",
    "eslint": "~8.57.0",
    "eslint-config-prettier": "~9.1.0",
    "prettier": "~3.5.1",
    "typescript": "~5.7.3",
    "vite": "~6.1.0",
    "vite-plugin-dts": "~4.5.0",
    "vitest": "~3.0.5"
  },
  "keywords": [
    "search",
    "performance",
    "indexing"
  ],
  "license": "MIT",
  "author": "Jonattan Vel√°squez <jonattanva89@gmail.com>",
  "files": [
    "dist"
  ],
  "main": "./dist/monolieta-search.umd.cjs",
  "types": "./dist/index.d.ts",
  "module": "./dist/monolieta-search.js",
  "exports": {
    ".": {
      "import": "./dist/monolieta-search.js",
      "require": "./dist/monolieta-search.umd.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jonattanva/monolieta-search.git"
  },
  "engines": {
    "node": ">=20.x"
  }
}
